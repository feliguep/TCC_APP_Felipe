<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.guedes.tcc_app.CadastroComumActivity">

    <TextView
        android:id="@+id/textView17"
        android:layout_width="189dp"
        android:layout_height="30dp"
        android:text="Responsável pelo registro: "
        android:textAppearance="@style/TextAppearance.AppCompat"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="29dp" />

    <EditText
        android:id="@+id/respon_reg"
        android:layout_width="168dp"
        android:layout_height="32dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="185dp"
        tools:layout_editor_absoluteY="29dp" />

    <EditText
        android:id="@+id/endereco"
        android:layout_width="183dp"
        android:layout_height="23dp"
        android:ems="10"
        android:inputType="text"
        tools:layout_editor_absoluteX="135dp"
        tools:layout_editor_absoluteY="84dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="81dp"
        android:layout_height="23dp"
        android:text="Endereço:"
        android:textAppearance="@style/TextAppearance.AppCompat"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="84dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CEP:"
        android:textAppearance="@style/TextAppearance.AppCompat"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="136dp" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Telefone:"
        android:textAppearance="@style/TextAppearance.AppCompat"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="189dp" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Celular:"
        android:textAppearance="@style/TextAppearance.AppCompat"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="251dp" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Login:"
        android:textAppearance="@style/TextAppearance.AppCompat"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="305dp" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Senha:"
        android:textAppearance="@style/TextAppearance.AppCompat"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="398dp" />

    <EditText
        android:id="@+id/editTextCEP"
        android:layout_width="210dp"
        android:layout_height="32dp"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="63dp"
        tools:layout_editor_absoluteY="128dp" />

    <EditText
        android:id="@+id/editTextCel"
        android:layout_width="208dp"
        android:layout_height="26dp"
        android:ems="10"
        android:inputType="phone"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="237dp" />

    <EditText
        android:id="@+id/editTextTel"
        android:layout_width="211dp"
        android:layout_height="31dp"
        android:ems="10"
        android:inputType="phone"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="175dp" />

    <EditText
        android:id="@+id/editTextLogin"
        android:layout_width="210dp"
        android:layout_height="33dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="98dp"
        tools:layout_editor_absoluteY="298dp" />

    <EditText
        android:id="@+id/editTextSenha"
        android:layout_width="207dp"
        android:layout_height="32dp"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="383dp" />

    <Button
        android:id="@+id/buttonCadas_inic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Finalizar cadastro inicial"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="457dp" />

</android.support.constraint.ConstraintLayout>
