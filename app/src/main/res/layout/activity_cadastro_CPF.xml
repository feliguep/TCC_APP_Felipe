<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.guedes.tcc_app.cadastro_CPF">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cadastro CPF"
        android:textAppearance="@style/TextAppearance.AppCompat"
        tools:layout_editor_absoluteX="137dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nome Completo:"
        android:textAppearance="@style/TextAppearance.AppCompat"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="82dp" />

    <EditText
        android:id="@+id/nome_cpf_cliente"
        android:layout_width="221dp"
        android:layout_height="25dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="137dp"
        tools:layout_editor_absoluteY="82dp" />

    <TextView
        android:id="@+id/CPF_Cliente "
        android:layout_width="37dp"
        android:layout_height="21dp"
        android:text="CPF:"
        android:textAppearance="@style/TextAppearance.AppCompat"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="133dp" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="246dp"
        android:layout_height="26dp"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="128dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Natureza Jurídica: "
        android:textAppearance="@style/TextAppearance.AppCompat"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="183dp" />

    <RadioButton
        android:id="@+id/radioButton_PF"
        android:layout_width="136dp"
        android:layout_height="27dp"
        android:text="Pessoa Física"
        tools:layout_editor_absoluteX="159dp"
        tools:layout_editor_absoluteY="183dp" />

    <EditText
        android:id="@+id/cidade_CPF"
        android:layout_width="206dp"
        android:layout_height="22dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="255dp" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cidade: "
        android:textAppearance="@style/TextAppearance.AppCompat"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="255dp" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Estado:"
        android:textAppearance="@style/TextAppearance.AppCompat"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="295dp" />

    <EditText
        android:id="@+id/editText10"
        android:layout_width="62dp"
        android:layout_height="27dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="295dp" />

    <Button
        android:id="@+id/button_proximo_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Próximo"
        tools:layout_editor_absoluteX="174dp"
        tools:layout_editor_absoluteY="341dp" />

</android.support.constraint.ConstraintLayout>
